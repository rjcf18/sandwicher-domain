{
    "name": "sandwicher/domain",
    "description": "A project to test some development concepts - Domain",
    "version": "0.0.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Fusco",
            "email": "ricardo.fusco2@gmail.com"
        }
    ],
    "config": {
        "bin-dir": "bin/",
        "platform": {
            "php": "8.0.0"
        }
    },
    "require": {
        "php": "8.0.0"
    },
    "autoload": {
        "psr-4": {
            "Sandwicher\\Domain\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "UnitTest\\": "test/unit/"
        }
    },
    "scripts": {
        "test": [
            "@unitTest"
        ],
        "unitTest": "bin/phpunit --configuration config/unit-test.xml --log-junit test/reports/unitReport.xml"
    }
}